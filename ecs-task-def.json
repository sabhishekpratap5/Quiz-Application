{
    "family": "quiz-application-abhishek",
    "executionRoleArn": "arn:aws:iam::022608205880:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::022608205880:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "quiz-application",
        "image": "022608205880.dkr.ecr.us-east-1.amazonaws.com/quiz-application-abhishek:latest",
        "cpu": 1024,
        "memory": 3072,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/quiz-application-abhishek",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "3072"
  }
  